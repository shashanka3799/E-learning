<tests>
       <test>
              <question>the format identifier '%i' is also used for __ data type.</question>
              <option1>char</option1>
              <option2>integer</option2>
              <option3>float</option3>
              <option4>double</option4>
              <answer>2</answer>
       </test>
       <test>
              <question>Which data type is most suitable for storing a number 65000 in a 32-bit system</question>
              <option1>signed short</option1>
              <option2>unsigned short</option2>
              <option3>long</option3>
              <option4>integer</option4>
              <answer>2</answer>
       </test>
       <test>
              <question>Which of the following is true about return type of functions in C?</question>
              <option1>Functions can return any type</option1>
              <option2>Functions can return any type except array and functions</option2>
              <option3>Functions can return any type except array, functions and union</option3>
              <option4>Functions can return any type except array, functions, function pointer and union</option4>
              <answer>2</answer>
       </test>
       <test>
              <question>Any C program</question>
              <option1>Must contain at least one function</option1>
              <option2>Need not contain any function</option2>
              <option3>Needs input data</option3>
              <option4>None of the above</option4>
              <answer>1</answer>
       </test>
 
       <test>
              <question>What is the size of an int data type?</question>
              <option1>4 bytes</option1>
              <option2>8 bytes</option2>
              <option3>Depends on the system/compiler</option3>
              <option4>Cannot be determined</option4>
              <answer>3</answer>
       </test>

